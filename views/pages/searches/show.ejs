<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../layouts/head') %>
</head>
<body>
    <header>
    <%- include('../layouts/header') %>
    </header>
    <main>
      <div class='container'>
        <% searchResults.forEach(result => { %>
          <div class='book'>
            <h2><% result.title %></h2>
            <img src='<% result.imageLink %>'>
            <p><% result.description %></p>
          </div>
        <% }) %>
      </div>  
    </main>
    <footer>

    </footer>
</body>
</html>